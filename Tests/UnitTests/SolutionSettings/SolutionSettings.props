<?xml version="1.0" encoding="utf-8"?>
<Project ToolsVersion="4.0" xmlns="http://schemas.microsoft.com/developer/msbuild/2003">
  <ImportGroup Label="PropertySheets" />
  <PropertyGroup Label="UserMacros">
    <Boost>D:\Code\Boost\boost_1_55_0\</Boost>
    <BoostLib>D:\Code\Boost\boost_1_55_0\stage\lib</BoostLib>
    <ArduinoRoot>D:\Code\HAS\Arduino\</ArduinoRoot>
  </PropertyGroup>
  <PropertyGroup>
    <LibraryPath>$(BoostLib);$(LibraryPath);$(SolutionDir)$(Configuration)</LibraryPath>
  </PropertyGroup>
  <ItemDefinitionGroup>
    <ClCompile>
      <AdditionalIncludeDirectories>$(Boost);$(ArduinoRoot)\Libraries</AdditionalIncludeDirectories>
    </ClCompile>
  </ItemDefinitionGroup>
  <ItemGroup>
    <BuildMacro Include="Boost">
      <Value>$(Boost)</Value>
      <EnvironmentVariable>true</EnvironmentVariable>
    </BuildMacro>
    <BuildMacro Include="BoostLib">
      <Value>$(BoostLib)</Value>
      <EnvironmentVariable>true</EnvironmentVariable>
    </BuildMacro>
    <BuildMacro Include="ArduinoRoot">
      <Value>$(ArduinoRoot)</Value>
    </BuildMacro>
  </ItemGroup>
</Project>